<div class="container">
  <div class="row">
    <div class="col-md-4">
      <ul class="nav navbar-nav">
        <li class="active">
          {{#link-to 'artists.new'}}
            <h1>Create Artist</h1>
            {{/link-to}}
        </li>
      </ul>
    </div>
  </div>

  <div class="row">
    <div class="col-md-4">
      {{#each model.artists as |artist|}}
        <div>
          {{#link-to 'artists.artist' artist.id}}
            {{artist.name}}
          {{/link-to}}
        </div>
      {{/each}}
    </div>
    <div class="col-md-8">
      {{outlet}}
    </div>
  </div>
</div>
